<h1>Listing parameters</h1>

<table>
  <tr>
    <th>Alpha2</th>
    <th>Alpha3</th>
    <th>Beta</th>
    <th>Gamma</th>
    <th>Phi2</th>
    <th>Phi3</th>
  </tr>

<% @parameters.each do |parameter| %>
  <tr>
    <td><%=h parameter.alpha2 %></td>
    <td><%=h parameter.alpha3 %></td>
    <td><%=h parameter.beta %></td>
    <td><%=h parameter.gamma %></td>
    <td><%=h parameter.phi2 %></td>
    <td><%=h parameter.phi3 %></td>
    <td><%= link_to 'Show', parameter %></td>
    <td><%= link_to 'Edit', edit_parameter_path(parameter) %></td>
    <td><%= link_to 'Destroy', parameter, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New parameter', new_parameter_path %>